table.Pack{
    border-collapse: collapse;
    border: none;
    font: normal 11px helvetica, verdana, arial, sans-serif;
    background-repeat: repeat;
    border-spacing: 1px;
    background-color:#FFFFFF;
}

table.Pack thead th{
    font: bold 10px helvetica, verdana, arial, sans-serif;
    border: none;
    text-align:center;
    background:#61AEFC;
    color:#FFFFFF;
    padding-top:4px;
    padding-bottom:4px;
}

table.Pack tfoot th, tfoot td{
    padding:5px;
    text-transform:uppercase;
    font-size:12px;
    border-top: 4px solid #004080; /* colo de lineas*/
    border-bottom: 4px solid #004080;
}

table.Pack a,
table.Pack a:visited {
    color:#000;
    text-decoration:none;
}
table.Pack a:hover {
    COLOR:#FF6600; TEXT-DECORATION: none; /* color para el link*/
}
table.Pack tr.row1 {
   background-color:#B3D9FF;
}
table.Pack tr.row2 {
    background-color:#E8F8FF;
}

table.Pack tr.fila {
    background-color:#eaeaea;
    COLOR:#000000;
    font-weight:bold
}

table.Pack tr.on {
    background-color:#FFFFFF;
    COLOR:#FF6600;
}
table.Pack td {
    padding:5px 9px 5px 0px;
    vertical-align:top;
    padding-left:20px
}
.Estilo5 {
    color: #FFFFFF
}
.Estilo4 {
    color: #FF6600;
    font-weight: bold;
}